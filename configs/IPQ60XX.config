# 目标平台配置
CONFIG_TARGET_qualcommax=y                    # 启用高通AX系列WiFi 6平台支持
CONFIG_TARGET_qualcommax_ipq60xx=y            # 启用IPQ60xx系列芯片支持（中端路由器常用）
CONFIG_TARGET_MULTI_PROFILE=y                 # 启用多设备配置文件，一次性编译多个设备固件

# 支持的设备列表 - 覆盖多个品牌和型号
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_alfa-network_ap120c-ax=y      # Alfa Network AP120C-AX商用AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_anysafe_e1=y                  # Anysafe E1设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_cambiumnetworks_xe3-4=y       # Cambium Networks XE3-4企业级设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_cmiot_ax18=y                  # 中国移动AX18定制路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_glinet_gl-ax1800=y            # GL-iNet GL-AX1800旅行路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_glinet_gl-axt1800=y           # GL-iNet GL-AXT1800高性能路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02=y            # 京东云无线宝RE-CS-02（雅典娜）
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02="luci-app-athena-led luci-i18n-athena-led-zh-cn"  # 为雅典娜设备特别添加LED控制应用
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-07=y            # 京东云无线宝RE-CS-07
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y            # 京东云无线宝RE-SS-01
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_kt_ar06-012h=y                # KT AR06-012H（韩国电信设备）
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_lg_gapd-7500=y                # LG GAPD-7500设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_link_nn6000-v1=y              # Link NN6000 v1版本
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_link_nn6000-v2=y              # Link NN6000 v2版本
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_linksys_mr7350=y              # Linksys MR7350家用路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_linksys_mr7500=y              # Linksys MR7500家用路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_netgear_wax214=y              # NETGEAR WAX214商用AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_netgear_wax610=y              # NETGEAR WAX610商用AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_netgear_wax610y=y             # NETGEAR WAX610y商用AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_qihoo_360v6=y                 # 360安全路由V6
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_redmi_ax5=y                   # 红米AX5路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_redmi_ax5-jdcloud=y           # 红米AX5京东云定制版
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_tplink_eap610od=y             # TP-Link EAP610户外AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_tplink_eap620-hd-v3=y         # TP-Link EAP620-HD v3企业AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_tplink_eap623od-hd-v1=y       # TP-Link EAP623OD-HD v1户外AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_tplink_eap625od-hd-v1=y       # TP-Link EAP625OD-HD v1户外AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_xiaomi_ax1800=y               # 小米AX1800路由器
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_yuncore_fap650=y              # YunCore FAP650商用AP
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_netgear_rbr350=y              # NETGEAR RBR350（Orbi卫星节点）
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_netgear_rbs350=y              # NETGEAR RBS350（Orbi卫星节点）
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_zn_m2=y                       # ZN M2设备

# 硬件驱动配置
CONFIG_ATH11K_MEM_PROFILE_512M=y              # ath11k WiFi 6驱动使用512MB内存配置（适用于大多数IPQ60xx设备）
CONFIG_NSS_FIRMWARE_VERSION_11_4=y            # 使用NSS网络子系统固件版本11.4（提供硬件加速）

# 应用程序配置
CONFIG_PACKAGE_luci-app-athena-led=m          # 编译为模块：雅典娜设备LED控制应用（可选安装）
CONFIG_PACKAGE_luci-i18n-athena-led-zh-cn=m   # 编译为模块：雅典娜LED应用的简体中文语言包（可选安装）
